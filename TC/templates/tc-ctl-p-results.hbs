<div class="prpanel-group prsidebar-body " style="display: none" data-no-cb>
    <div class="prpanel prpanel-default">
        <div class="prpanel-heading">
            <h4 class="prpanel-title">
                <span class="prpanel-title-text"></span>
                <button class="tc-icon-btn tc-ctl-p-results-share-btn" hidden title="{{i18n "shareQuery"}}">{{i18n "shareQuery"}}</button>
                <button class="tc-icon-btn tc-ctl-p-results-dwn-btn prcollapsed-slide-submenu-dwn" hidden title="{{i18n "download"}}">{{i18n "download"}}</button>
                <button class="tc-icon-btn tc-ctl-p-results-csv-btn prcollapsed-slide-submenu-csv" hidden title="{{i18n "export.excel"}}">{{i18n "export.excel"}}</button>
                <button class="tc-icon-btn tc-ctl-p-results-min-btn prcollapsed-slide-submenu-min" title="{{i18n "hide"}}">{{i18n "hide"}}</button>
                <button class="tc-icon-btn tc-ctl-p-results-close-btn prcollapsed-slide-submenu-close" title="{{i18n "close"}}">{{i18n "close"}}</button>
            </h4>
        </div>
        <div id="results" class="prpanel-collapse collapse in">
            <div class="tc-ctl-p-results-menu"></div>
            <div class="prpanel-body list-group tc-ctl-p-results-info"></div>
            <div class="prpanel-body list-group tc-ctl-p-results-table"></div>
            <div class="prpanel-body list-group tc-ctl-p-results-chart"></div>
        </div>
        <div class="tc-ctl-p-results-resize-handler tc-resizable-grid-handlerRight"></div>
        <div class="tc-ctl-p-results-resize-handler tc-resizable-grid-handlerBottom"></div>
        <div class="tc-ctl-p-results-resize-handler tc-resizable-grid-handlerDiagonal"></div>
    </div>
</div>
<div class="prcollapsed prcollapsed-max prcollapsed-pull-left" style="display: none;" title="{{i18n "expand"}}" data-no-cb>
    <i class="fa-list-alt" hidden></i>
    <i class="fa-area-chart" hidden></i>
</div>